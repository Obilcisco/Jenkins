echo "=============Build Started============="
cat <<EOF > index.html
<html>
<body bgcolor=black>
<center>
<h2><font color=yellow>Hello from BOI</font></h2>
<font color=blue>www.biletskyi.pp.ua</font>
</center>
</body>
</html>
EOF
echo "=============Build Finished============="


echo "============Test Started==============="
result=`grep "Hello" index.html | wc -l`
echo $result
if [ "$result" = "1" ]
then
	echo "Test Passed"
else
	echo "Test Failed"
    exit 1
fi
echo "============Test Finished==============="